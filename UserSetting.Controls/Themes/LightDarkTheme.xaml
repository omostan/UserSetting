<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="{x:Static SystemColors.WindowBrushKey}" Color="WhiteSmoke" />
    <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#5F5C5C" />

    <!--  Header  -->
    <Color x:Key="HeadBackgroundBrush1">#2E2A2A</Color>
    <Color x:Key="HeadBackgroundBrush2">#262626</Color>

    <!--  Primary Button  -->
    <SolidColorBrush x:Key="PrimaryButtonMouseOver" Color="#5F5C5C" />

    <!--  Primary Button Version Info  -->
    <SolidColorBrush x:Key="PrimaryButtonVersionInfo" Color="#343333" />

    <!--  Buttons  -->
    <SolidColorBrush x:Key="DefaultBackgroundColor" Color="#0A0A0A" />
    <SolidColorBrush x:Key="RedColor" Color="Red" />
    <SolidColorBrush x:Key="YellowColor" Color="Yellow" />
    <SolidColorBrush x:Key="MouseOverForegroundColor" Color="White" />
    <SolidColorBrush x:Key="HeaderForegroundColor" Color="#BEBDBD" />

    <!--  Minimize Button  -->
    <SolidColorBrush x:Key="MinimizeButtonForegroundColor" Color="Sienna" />
    <SolidColorBrush x:Key="MinimizeButtonMouseOverBackgroundColor" Color="#939494" />

    <!--  Maximize Button  -->
    <SolidColorBrush x:Key="MaximizeButtonForegroundColor" Color="#9A9E9E" />
    <SolidColorBrush x:Key="MaximizeButtonMouseOverBackgroundColor" Color="#939494" />

    <!--  Footer  -->
    <Color x:Key="FootBackgroundBrush1">#242B34</Color>
    <Color x:Key="FootBackgroundBrush2">#46505D</Color>
    <SolidColorBrush x:Key="FooterForegroundColor" Color="#CECFCF" />

    <!--  Custom Window Background  -->
    <SolidColorBrush x:Key="DefaultForeground" Color="#5F5C5C" />
    <SolidColorBrush x:Key="CustomWindowBackground" Color="White" />

    <!--  ComboBox Foreground / Background  -->
    <SolidColorBrush x:Key="ComboboxForeground" Color="#070C14" />
    <SolidColorBrush x:Key="ComboboxBackground" Color="WhiteSmoke" />

    <!--  Text Foreground / Background  -->
    <SolidColorBrush x:Key="TextForeground" Color="#070C14" />
    <SolidColorBrush x:Key="TextBackground" Color="Transparent" />

    <!--  Custom Window BorderBrush  -->
    <SolidColorBrush x:Key="CustomWindowBorderBrush" Color="#191919" />

    <!--  BorderPrimary  -->
    <SolidColorBrush x:Key="BorderPrimary" Color="#BABDBD" />

    <!--  GroupBoxPrimary  -->
    <SolidColorBrush x:Key="GroupBoxPrimary" Color="#404040" />

    <!--  LanguageForegroundColor  -->
    <SolidColorBrush x:Key="LanguageForegroundColor" Color="#5F5C5C" />

    <!--  ComboboxBorder  -->
    <SolidColorBrush x:Key="ComboboxBorder" Color="#706B6B" />

    <!--  Message Borderline  -->
    <SolidColorBrush x:Key="MessageBorderLine" Color="#E1E1E1" />

    <!--  FilterWatermark  -->
    <SolidColorBrush x:Key="FilterWatermarkForeground" Color="#5C5C5C" />

    <!--  BusyIndicator  -->
    <SolidColorBrush x:Key="IndicatorForeground" Color="#0A0A0A" />
    <SolidColorBrush x:Key="IndicatorBackground" Color="WhiteSmoke" />

    <!--  Edit Popup Window  -->
    <SolidColorBrush x:Key="ModalTitleForeground" Color="#BEBDBD" />
    <SolidColorBrush x:Key="ModalTitleLowerBorderLine" Color="#E1E1E1" />

    <!--  GroupBy Filter  -->
    <SolidColorBrush x:Key="GroupByBackground" Color="#9F9D9D" />
    <SolidColorBrush x:Key="GroupByForeground" Color="#282727" />

    <!--  Theme Toggle Button  -->
    <SolidColorBrush x:Key="ThemeToggleButtonBackgroundColor" Color="#918E8E" />
    <SolidColorBrush x:Key="ThemeToggleButtonForegroundColor" Color="#035A7F" />
    <SolidColorBrush x:Key="ThemeToggleButtonMouseOverBackgroundColor" Color="#939494" />

    <!--  SubPosition Message  -->
    <SolidColorBrush x:Key="SubPositionMessageForegroundColor" Color="#070C14" />

    <!--  SelectedItemHighlightColors  -->
    <Color x:Key="SelectedItemHighlightBackgroundColor">#575353</Color>
    <Color x:Key="SelectedHighlightTextForeground">#FFFFFF</Color>

    <SolidColorBrush x:Key="SelectedItemBackgroundColor" Color="#575353" />
    <SolidColorBrush x:Key="SelectedItemForegroundColor" Color="#FFFFFF" />

    <!--  Sub-Position Button  -->
    <SolidColorBrush x:Key="SelectedSubPositionButtonHighlightBackgroundColor" Color="#575353" />
    <SolidColorBrush x:Key="UnSelectedSubPositionButtonBackgroundColor" Color="#262626" />

    <SolidColorBrush x:Key="GridViewColumnHeaderSortGlyphDarkBrush" Color="#404040" />
    <SolidColorBrush x:Key="GridViewColumnHeaderSortGlyphLightBrush" Color="#5B5A5A" />

    <!--  SelectedItemHighlightBackgroundColor  -->
    <SolidColorBrush x:Key="MainMenuItemForegroundColor" Color="#000000" />
    <SolidColorBrush x:Key="MainMenuForegroundColor" Color="#000000" />
    <SolidColorBrush x:Key="MainMenuBackgroundColor" Color="#FFFFFF" />

</ResourceDictionary>