    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <!--  ThemeToggleButton Content  -->
    <s:String x:Key="KeyBlue">Blau</s:String>
    <s:String x:Key="KeyGreen">Grün</s:String>

