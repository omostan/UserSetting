<ResourceDictionary
    x:Class="UserSetting.WPF.Resources.Languages.en_GB"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <!--  ThemeToggleButton Content  -->
    <s:String x:Key="KeyBlue">Blue</s:String>
    <s:String x:Key="KeyGreen">Green</s:String>

    <!--  Buttons ToolTips  -->
    <s:String x:Key="KeyCancel">Cancel</s:String>
    <s:String x:Key="KeyDelete">Delete</s:String>
    <s:String x:Key="KeyReset">Reset</s:String>
    <s:String x:Key="KeySave">Save</s:String>
    <s:String x:Key="KeyClose">Close</s:String>
    <s:String x:Key="KeyMinimize">Minimize</s:String>
    <s:String x:Key="KeyMaximize">Maximize</s:String>
    <s:String x:Key="KeyTheme">Themes</s:String>

    <!--  Current Version Info  -->
    <s:String x:Key="KeyVersionInfoUpdate">Version Info Update</s:String>
    <s:String x:Key="KeyVersionInfoLeft">A new version</s:String>
    <s:String x:Key="KeyVersionInfoRight">is available. Please restart FAME and SWIM to get the updated version.</s:String>

</ResourceDictionary>
